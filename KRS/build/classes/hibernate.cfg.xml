<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/krs</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.show_sql">true</property>
    <property name="hibernate.format_sql">true</property>
    <property name="cache.use_second_level_cache">true</property>
    <property name="hibernate.connection.password">yyaayyaatt</property>
    <mapping class="com.wordpress.yyaayyaatt.model.DetailKrs"/>
    <mapping class="com.wordpress.yyaayyaatt.model.Dosen"/>
    <mapping class="com.wordpress.yyaayyaatt.model.Kelas"/>
    <mapping class="com.wordpress.yyaayyaatt.model.Mahasiswa"/>
    <mapping class="com.wordpress.yyaayyaatt.model.Mk"/>
    <mapping class="com.wordpress.yyaayyaatt.model.Pembimbing"/>
    <mapping class="com.wordpress.yyaayyaatt.model.WaktuKrs"/>
    <mapping class="com.wordpress.yyaayyaatt.model.Login"/>
  </session-factory>
</hibernate-configuration>
